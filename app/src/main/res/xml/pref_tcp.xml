<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:key="pref_tcp_ip"
        android:title="@string/pref_tcp_ip_title"
        android:summary="@string/pref_tcp_ip_summary"
        android:defaultValue="192.168.188.20"   />

    <EditTextPreference
        android:key="pref_tcp_port"
        android:title="@string/pref_tcp_port_title"
        android:summary="@string/pref_tcp_port_summary"
        android:defaultValue="5556"   />

    <EditTextPreference
        android:key="pref_tcp_timeout"
        android:title="@string/pref_tcp_timeout_title"
        android:summary="@string/pref_tcp_timeout_summary"
        android:defaultValue="1000"   />
</PreferenceScreen>